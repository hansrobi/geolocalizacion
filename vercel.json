{
    "version": 2,
    "builds": [
        {
            "src": "server/package.json",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/server/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/client/public/$1"
        }
    ]
}
